{
   "name": "react-vite-trpc-server",
   "version": "0.0.1",
   "private": true,
   "scripts": {
      "dev": "nodemon src/server.ts ",
      "start": "cross-env NODE_ENV=production node dist/src/server.js",
      "pm2:start": "pnpm pm2:delete && pnpm build && cross-env NODE_ENV=development pm2 start dist/src/server.js --name vite-trpc-server",
      "pm2:delete": "pm2 delete all &",
      "pm2:logs": "pm2 logs",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "ts:check": "tsc -b",
      "check": "pnpm lint && pnpm ts:check",
      "build": "tsc -b",
      "test:unit": "vitest .unit.test.ts",
      "test:unit:run": "pnpm test:unit --run",
      "test:integration": "vitest .integration.test.ts",
      "test:integration:run": "pnpm test:integration --run",
      "test:e2e": "vitest .e2e.test.ts",
      "test:e2e:run": "pnpm test:e2e --run",
      "test:coverage": "pnpm pm2:start && vitest run --coverage && pnpm pm2:delete",
      "test:coverage:preview": "pnpm test:coverage && vite preview --outDir ./coverage --open",
      "test": "jest"
   },
   "dependencies": {
      "@trpc/client": "11.0.0-rc.800",
      "@trpc/server": "11.0.0-rc.800",
      "bcryptjs": "^3.0.2",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "devalue": "^5.1.1",
      "express": "^4.18.2",
      "express-serve-static-core": "^0.1.1",
      "ioredis": "^5.5.0",
      "moment": "^2.30.1",
      "mysql2": "^3.12.0",
      "pino": "^9.6.0",
      "pino-multi-stream": "^6.0.0",
      "rotating-file-stream": "^3.2.6",
      "sequelize": "^6.37.5",
      "superjson": "^2.2.2",
      "uuid": "^11.1.0",
      "zod": "^3.24.2"
   },
   "devDependencies": {
      "@babel/core": "^7.26.9",
      "@babel/preset-env": "^7.26.9",
      "@swc/core": "^1.3.62",
      "@trivago/prettier-plugin-sort-imports": "^4.2.0",
      "@types/cookie-parser": "^1.4.3",
      "@types/express": "^4.17.17",
      "@types/fs-extra": "^11.0.1",
      "@types/jest": "^29.5.14",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^18.16.17",
      "@vitest/coverage-istanbul": "^0.34.1",
      "babel-jest": "^29.7.0",
      "cross-env": "^7.0.3",
      "dotenv": "^16.1.4",
      "envalid": "^7.3.1",
      "fs-extra": "^11.1.1",
      "jest": "^29.7.0",
      "jest-environment-jsdom": "^29.7.0",
      "module-alias": "^2.2.3",
      "nodemon": "^3.1.9",
      "pino-pretty": "^13.0.0",
      "pm2": "^5.3.0",
      "ts-jest": "^29.2.5",
      "ts-node": "^10.9.1",
      "tsup": "^8.3.6",
      "typescript": "^5.1.6",
      "vite-tsconfig-paths": "^4.2.0",
      "vitest": "^0.34.1"
   }
}
